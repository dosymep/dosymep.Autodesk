<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net48</TargetFramework>
        <Authors>dosymep</Authors>
        <RepositoryType>Git</RepositoryType>
        <RepositoryUrl>https://github.com/dosymep/dosymep.Autodesk</RepositoryUrl>
        <PackageProjectUrl>https://github.com/dosymep/dosymep.Autodesk</PackageProjectUrl>
        <PackageTags>dosymep, autodesk, revit, navisworks, bim360</PackageTags>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <DocumentationFile>bin\Debug\dosymep.Autodesk.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DocumentationFile>bin\Release\dosymep.Autodesk.xml</DocumentationFile>
    </PropertyGroup>

</Project>
